    "angular-animate": "~1.2",
    "angular-resource": "~1.2",
    "angular-cookies": "~1.2",
    "angular-mocks": "~1.2",
    "angular-ui-utils": "~0.1",
    "angular-sanitize": "~1.3.7",
    "moment": "~2.5",
    "less.js": "~1.6",
    "font-awesome": "~4.0",
    "ngtoast": "~1.5.0",
    "json3": "~3.3.2",
    "es5-shim": "~4.0.5",
    "html5shiv": "~3.7.2",
    "respond": "~1.4.2",
    "angular-utils-pagination": "~0.5.0"
  },
  "resolutions": {
    "angular": "1.2.28",
