{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.0",
  "main": "index.html",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "devDependencies": {
    "jquery": "~1.9",
    "angular": "~1.2",
    "underscore": "~1.5",
    "bootstrap": "~3.3.5",
    "angular-bootstrap": "~0.10",
    "cpp-ui": "latest",
    <% if(uirouter) { print('"angular-ui-router": "~0.2",'); } else { print('"angular-route": "~1.2",'); } %>
